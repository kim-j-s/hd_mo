{{!-- 
	id : id 배정
	labelname : label 배정
--}}
<div class="inp_checkbox">
	<input type="checkbox"
		id="{{id}}"
		{{#if chk_class}}class="{{chk_class}}"{{/if}}
		{{#if name}}name="{{name}}"{{/if}}
		{{#if checked}}checked{{/if}}
		{{#if disabled}}disabled{{/if}}
		{{#if ag}} title="체크하시면 {{{labelname}}} 약관 팝업이 활성화 됩니다."{{/if}}
		{{#if ag_more}} title="체크하시면 {{{labelname}}} 약관 팝업이 활성화 됩니다."{{/if}}
		{{#if ag_top}} title="체크하시면 {{{labelname}}} 약관 팝업이 활성화 됩니다."{{/if}}
		{{!-- {{#if exp}} aria-labelledby="{{id}}_desc"{{/if}} --}}
	>
	<label for="{{id}}" class="chk_label{{#if label_class}} {{label_class}}{{/if}}{{#if ag_more}} more_add{{/if}}{{#if ag_top}} chk_label_ty2{{/if}}"><span class="label_cont">{{{labelname}}}</span></label>
	{{#if exp}}
	{{!-- <div class="chk_lby" id="{{id}}_desc"> --}}
	<div class="chk_lby">
		{{> @partial-block }}
	</div>
	{{else}}
	{{#if @partial-block}}{{> @partial-block }}{{/if}}
	{{/if}}
	{{#if ag_more}}
	<button type="button" class="ag_more_btn" aria-label="{{{labelname}}} 자세히 보기">자세히</button>
	{{/if}}
</div>